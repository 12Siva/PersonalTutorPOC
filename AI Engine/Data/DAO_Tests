import unittest
import OpenAIDao

class DAOTests(unittest.TestCase, OpenAIDao):

    def test_transport_text_happy_path(self):
        self.assertEqual('foo'.upper(), 'FOO')
   
   def test_transport_text_expected_failure_path(self):
       self.assertEqual('foo'.upper(), 'foo')
       
   def __init__(self, unittest.TestCase):
        self.r = OpenAIDao

if __name__ == '__main__':
    unittest.main()
